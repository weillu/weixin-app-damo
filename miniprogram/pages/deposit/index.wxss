/* pages/pay/index.wxss */
.recharge{
  background:#ffffff;
}
.recharge-box{
  padding:0 30rpx;
}
.recharge-box .title{
  font-size: 44rpx;
  line-height: 80rpx;
  height: 100rpx;
  text-align: center;
  color: #565E61;
  margin-top:20rpx;
}
.recharge-box .input-box{
  text-align:center;
  margin-top:50rpx;
  margin-bottom:30rpx;
}

.item-num{
  display:flex;
  justify-content: space-between;
  margin-bottom: 60rpx;
  
}
.item-num .num{
  height: 290rpx;
  background-image: url(data:image/png;base64,);
  background-position:0% 0%;
  background-repeat:no-repeat;
  background-size:cover;
  color: #333333;
  font-size: 32rpx;
  width: 336rpx;
  text-align: center;
}

.item-num span {
  display: block;
}
.t1 {
  padding-top:46rpx;
  font-size: 44rpx;
  line-height: 60rpx;
  height: 60rpx;
  color: #000000;
}
.t2 {
  padding-top:12rpx;
  font-size: 36rpx;
  line-height: 50rpx;
  height: 50rpx;
  color: #000000;
}
.t3 {
  font-size: 28rpx;
  line-height: 40rpx;
  height: 40rpx;
  color: #C48650;
  width: 220rpx;
  border-radius: 10rpx;
  border:2rpx solid #d09d73;
  box-shadow:0rpx 5rpx 10rpx 0rpx #d09d73;
  background: #FFF7F0;
  text-align:center;
  margin: 20rpx auto;
}

.item-num .num.checked .t3{
  background: #E72C43;
  color: #FFFFFF;
}

